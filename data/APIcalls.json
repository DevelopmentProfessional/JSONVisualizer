{
  "apiCalls": [
    {
      "name": "Mirth Login",
      "url": "{{MirthBaseURL}}/users/_login",
      "method": "POST",
      "body": "username={{MirthDevUsername}}&password={{MirthDevPassword}}",
      "headers": {
        "Content-Type": "application/x-www-form-urlencoded",
        "X-Requested-With": "OpenAPI"
      },
      "variables": [
        "baseURL",
        "Username",
        "Password"
      ],
      "checked": true,
      "isLogin": true,
      "loginVar": "sessionId",
      "outputFileName": "mirth-login"
    },
